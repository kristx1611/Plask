<resources xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- Base application theme. -->
    <style name="Theme.Team21" parent="Theme.MaterialComponents.DayNight.NoActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/standard_green</item>
        <item name="colorPrimaryVariant">@color/tinted_green</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/tinted_green</item>
        <item name="colorSecondaryVariant">@color/teal_700</item>
        <item name="colorOnSecondary">@color/black</item>
        <!-- Status bar color. -->
        <item name="android:windowTranslucentStatus">true</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="fasiliteter" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#DDE3DD</item>
        <item name="android:background">#931C1C</item>
    </style>
